---
path: <%= projectName %>/package.json
---
{
  "name": "<%= projectName %>",
  "version": "1.0.0",
  "description": "please enter the module description.",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/es/index.js",
      "require": "./dist/cjs/index.js",
      "types": "./dist/types/index.d.ts"
    },
    "./Button": {
      "import": "./dist/es/Button/index.js",
      "require": "./dist/cjs/Button/index.js",
      "types": "./dist/types/Button.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "homepage": "https://github.com/<%= username %>/<%= projectName %>#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/<%= username %>/<%= projectName %>.git"
  },
  "bugs": {
    "url": "https://github.com/<%= username %>/<%= projectName %>/issues"
  },
  "scripts": {
    "dev": "vite build --watch",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "format": "prettier --write ."
  },
  "keywords": [],
  "author": "<%= username %> <<%= email%>>",
  "license": "MIT",
  "devDependencies": {
    "typescript": "^5.3.3",
    "vite": "^5.0.11",
    "vite-plugin-dts": "^3.7.1",
    "prettier-plugin-organize-imports": "^3.2.4",
    "rollup": "^4.9.5",
    "@types/node": "^20.11.4",
    "@vitejs/plugin-vue": "^5.0.3",
    "fast-glob": "^3.3.2",
    "change-case": "^5.4.2"
  },
  "dependencies": {
    "vue": "^3.4.14"
  }
}